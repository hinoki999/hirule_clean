{
    "agents": {
        "protocol": {
            "mqtt": {
                "host": "localhost",
                "port": 1883
            },
            "coap": {
                "host": "localhost",
                "port": 5683
            },
            "http3": {
                "host": "localhost",
                "port": 443
            }
        },
        "blockchain": {
            "rollup_batch_size": 100,
            "validation_threshold": 0.75
        },
        "semantic": {
            "model_name": "bert-base-uncased",
            "compression_ratio": 0.5,
            "priority_threshold": 0.7
        }
    },
    "network": {
        "max_connections": 1000,
        "timeout": 30,
        "retry_attempts": 3
    },
    "security": {
        "encryption_enabled": true,
        "auth_required": true,
        "token_expiry": 3600
    }
}